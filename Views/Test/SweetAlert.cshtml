
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>

<button class="btn btn-primary" id="btn1">Buton1</button>
<button class="btn btn-primary" id="btn2">Buton2</button>

<script>
    $('#btn1').click(function (){
        swal("Mvc Yemek Sitesi Projesi");
    });
</script>

<script>
    $('#btn2').click(function (){
        swal("Tebrikler! Başarılı bir şekilde eklendi.", "success");
    });
</script>